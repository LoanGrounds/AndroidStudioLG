<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Navbar.Footer">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1">

        <ImageButton
            android:id="@+id/imUser"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="104dp"
            android:layout_marginEnd="40dp"
            android:layout_weight="1"
            android:background="#FFFFFF"
            android:src="@drawable/user"
            app:layout_constraintBottom_toBottomOf="@+id/imBtnSol"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/imBtnSol"
            app:layout_constraintTop_toTopOf="@+id/imBtnSol" />

        <ImageButton
            android:id="@+id/imBtnHouse"
            android:layout_width="26dp"
            android:layout_height="30dp"
            android:layout_marginStart="40dp"
            android:layout_marginTop="12dp"
            android:layout_weight="1"
            android:background="#FFFFFF"
            android:src="@drawable/home"
            app:layout_constraintEnd_toStartOf="@+id/imBtnSol"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageButton
            android:id="@+id/imBtnSol"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="112dp"
            android:layout_weight="1"
            android:background="#FFFFFF"
            android:src="@drawable/solicitados"
            app:layout_constraintBottom_toBottomOf="@+id/imBtnHouse"
            app:layout_constraintEnd_toStartOf="@+id/imUser"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/imBtnHouse"
            app:layout_constraintTop_toTopOf="@+id/imBtnHouse" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>